<!DOCTYPE html>
<html>
  <head>
    <title>InfoVisLanguages</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= javascript_tag "window._token = '#{form_authenticity_token}'" %>
    <%= stylesheet_pack_tag 'application', turbolink_track: true %>
